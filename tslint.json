{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "radix": false,
    "one-variable-per-declaration": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "ordered-imports": false,
    "member-access": false,
    "interface-name": false,
    "member-ordering": false,
    "object-literal-sort-keys": false
  }
}
